<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Mobile App Prepaid Regression - Bundle Purchase - I Prepaid Rate Plan">
    <listeners>
        <listener class-name="com.umtest.testframe.listener.ExtentTestNGITestListener"></listener>
    </listeners>

<!-- ***************************************  I Prepaid Rate Plan  ************************************************-->

        <test name="[Mobile Apps Prepaid] Bundle Purchase I40 - I Prepaid Plan">
            <parameter name="msisdn" value="601128188262"/>
            <parameter name="bundleName" value="i40"/>
            <parameter name="accountType" value="Prepaid"/>
            <classes>
                <class name="com.umtest.mobileapps.testcases.MobileAppBundlePurchase">
                    <methods>
                        <include name="MobileAppPurchaseBundle" />
                    </methods>
                </class>
            </classes>
        </test>

        <test name="[Mobile Apps Prepaid] Bundle Purchase EPIKKK Video3 - I Prepaid Plan">
        <parameter name="msisdn" value="60183592234"/>
        <parameter name="bundleName" value="EPIKKK Video3"/>
        <parameter name="accountType" value="Prepaid"/>
        <classes>
            <class name="com.umtest.mobileapps.testcases.MobileAppBundlePurchase">
                <methods>
                    <include name="MobileAppPurchaseBundle" />
                </methods>
            </class>
        </classes>
        </test>
</suite>