<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Mobile App Prepaid Regression">
    <listeners>
        <listener class-name="com.umtest.testframe.listener.ExtentTestNGITestListener"></listener>
    </listeners>

    <!-- **************************  MB Prepaid Rate Plan  **************************-->

    <test name="[Mobile Apps Prepaid] Bundle Purchase - MB40 - Booster Plan - MB Prepaid">
        <parameter name="msisdn" value="60182146929"/>
        <parameter name="bundleName" value="MB40"/>
        <parameter name="accountType" value="Prepaid"/>
        <parameter name="testDataFile" value="bundlePurchase/mbPrepaid/BoosterPlan-MB40.json"/>
        <classes>
            <class name="com.umtest.mobileapps.testcases.MobileAppBundlePurchase">
                <methods>
                    <include name="MobileAppPurchaseBundleBooster" />
                </methods>
            </class>
        </classes>
    </test>

